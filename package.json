{
  "name": "svelte-store-monad",
  "version": "1.0.0",
  "license": "MIT",
  "author": "nasso",
  "description": "Monadic interface for Svelte stores",
  "keywords": [
    "svelte",
    "svelte.js",
    "sveltejs",
    "svelte3",
    "svelte-store",
    "monad",
    "monadic",
    "store",
    "state management",
    "functional",
    "functional programming",
    "fp",
    "bind",
    "sequence"
  ],
  "scripts": {
    "package": "svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.2.2",
    "@vitest/ui": "^0.34.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "publint": "^0.2.2",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
